{
  "name": "dataplex",
  "version": "0.1.0",
  "description": "Connect to Dataplex",
  "mcpServers": {
    "Dataplex": {
      "command": "${extensionPath}${/}toolbox",
      "args": [
        "--prebuilt",
        "dataplex",
        "--stdio"
      ],
      "env": {
        "DATAPLEX_PROJECT": "${DATAPLEX_PROJECT}"
      }
    }
  },
  "contextFileName": "DATAPLEX.md"
}